{
    "name": "redsnapper/laravel-doorman",
    "description": "Will the doorman let you in? Only if you've got the right set of permissions.",
    "type": "laravel",
    "license": "mit",
    "authors": [
        {
            "name": "param",
            "email": "param@redsnapper.net"
        },
        {
            "name": "mark",
            "email": "mark@redsnapper.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": "^7.1.3",
      "illuminate/support": ">=5.6",
      "illuminate/database": ">=5.6"
    },
    "autoload": {
      "psr-4": {
        "Redsnapper\\LaravelDoorman\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Redsnapper\\LaravelDoorman\\Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^7.0",
      "mockery/mockery": "^1.0",
      "orchestra/testbench": "3.8.x-dev"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Redsnapper\\LaravelDoorman\\DoormanServiceProvider",
          "Redsnapper\\LaravelDoorman\\DoormanAuthServiceProvider"
        ]
      }
    }
}
