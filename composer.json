{
    "name": "rs/laravel-doorman",
    "description": "Will the doorman let you in? Only if you've got the right set of permissions.",
    "type": "laravel",
    "license": "mit",
    "authors": [
        {
            "name": "param",
            "email": "param@redsnapper.net"
        },
        {
            "name": "mark",
            "email": "mark@redsnapper.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": "^7.3.0",
      "illuminate/support": "^5.8|^6.0",
      "illuminate/database": "^5.8|^6.0"
    },
    "autoload": {
      "psr-4": {
        "Redsnapper\\LaravelDoorman\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Redsnapper\\LaravelDoorman\\Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "mockery/mockery": "^1.0",
      "orchestra/testbench": "^3.8|^4.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Redsnapper\\LaravelDoorman\\DoormanServiceProvider"
        ]
      }
    }
}
